{
	/* Begin Not So Minimal - VS Code Settings */
	"catppuccin.syncWithIconPack": true,
	"catppuccin.workbenchMode": "flat",
	"chat.commandCenter.enabled": false,
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.acceptSuggestionOnEnter": "on",
	"editor.cursorBlinking": "smooth",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.foldingImportsByDefault": false,
	"editor.fontFamily": "JetBrainsMono Nerd Font, JetBrains Mono, FiraCode Nerd Font, Fira code, monospace",
	"editor.fontLigatures": true,
	"editor.fontSize": 16,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": true,
	"editor.hover.delay": 700,
	"editor.indentSize": 4,
	"editor.inlayHints.fontFamily": "JetBrainsMono Nerd Font, JetBrains Mono, FiraCode Nerd Font, Fira code, monospace",
	"editor.inlineSuggest.fontFamily": "JetBrainsMono Nerd Font, JetBrains Mono, FiraCode Nerd Font, Fira code, monospace",
	"editor.inlineSuggest.suppressSuggestions": true,
	"editor.inlineSuggest.syntaxHighlightingEnabled": false,
	"editor.largeFileOptimizations": false,
	"editor.lineHeight": 1.7,
	"editor.linkedEditing": true,
	"editor.minimap.enabled": false,
	"editor.quickSuggestions": {
		"comments": "off",
		"other": "off",
		"strings": "off"
	},
	"editor.quickSuggestionsDelay": 700,
	"editor.renderLineHighlight": "gutter",
	"editor.scrollbar.horizontal": "auto",
	"editor.scrollbar.vertical": "auto",
	"editor.semanticHighlighting.enabled": true,
	"editor.semanticTokenColorCustomizations": {
		"enabled": true
	},
	"editor.smoothScrolling": true,
	"editor.suggest.filterGraceful": true,
	"editor.suggest.insertMode": "replace",
	"editor.suggest.matchOnWordStartOnly": false,
	"editor.suggest.preview": false,
	"editor.suggest.selectionMode": "never",
	"editor.suggestOnTriggerCharacters": true,
	"editor.tabCompletion": "off",
	"editor.tokenColorCustomizations": {
		"textMateRules": []
	},
	"editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
	"explorer.compactFolders": false,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.confirmPasteNative": false,
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.patterns": {
		"*.ahk": "${capture}.d.ahk",
		"*.cs": "${capture}.*",
		"*.csproj": "${capture}.csproj*",
		"*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
		"*.jsx": "${capture}.js",
		"*.ts": "${capture}.js, ${capture}.d.ts, ${capture}.js.map, ${capture}.min.js",
		"*.tsx": "${capture}.ts",
		"Cargo.toml": "Cargo.*, tauri.conf.*",
		"package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, tsconfig*, .eslintrc*, eslint.config.*, .prettierrc*, .babelrc*, pnpm-workspace.*, .yarnrc*, .npmrc*, tailwind.config.*, postcss.config.*, vite.config.*, prettier.config.*, rsbuild.*, .prettierignore, .eslintignore, .pnp.cjs, .pnp.loader.mjs, codegen.ts, .graphqlrc*, next.config.*, apollo.config.*, graphql.config.*",
		"deno.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, tsconfig*, .eslintrc*, eslint.config.*, .prettierrc*, .babelrc*, pnpm-workspace.*, .yarnrc*, .npmrc*, tailwind.config.*, postcss.config.*, vite.config.*, prettier.config.*, rsbuild.*, .prettierignore, .eslintignore, .pnp.cjs, .pnp.loader.mjs, codegen.ts, .graphqlrc*, next.config.*, apollo.config.*, graphql.config.*, deno.lock",
		"deno.jsonc": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, tsconfig*, .eslintrc*, eslint.config.*, .prettierrc*, .babelrc*, pnpm-workspace.*, .yarnrc*, .npmrc*, tailwind.config.*, postcss.config.*, vite.config.*, prettier.config.*, rsbuild.*, .prettierignore, .eslintignore, .pnp.cjs, .pnp.loader.mjs, codegen.ts, .graphqlrc*, next.config.*, apollo.config.*, graphql.config.*, deno.lock"
	},
	"extensions.ignoreRecommendations": true,
	"files.associations": {
		".env*": "dotenv",
		"${workspaceFolder}/settings/settings.json": "jsonc"
	},
	"files.exclude": {
		"**/.DS_Store": true,
		"**/.yarn": true,
		"**/node_modules": true
	},
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"git.openRepositoryInParentFolders": "always",
	"gruvbox-material-icons.files.associations": {
		"tsconfig.dev.json": "tsconfig"
	},
	"gruvbox-material-icons.folders.associations": {
		"adapters": "contract",
		"api.server": "api",
		"domain": "class",
		"dtos": "typescript",
		"entities": "class",
		"eslint-config": "tools",
		"factories": "class",
		"fakes": "mock",
		"gql.server": "graphql",
		"graphql.server": "graphql",
		"grpc": "pipe",
		"http": "container",
		"implementations": "core",
		"infra": "app",
		"kafka": "pipe",
		"kube": "kubernetes",
		"mappers": "meta",
		"migrations": "tools",
		"presentation": "template",
		"protocols": "contract",
		"providers": "include",
		"renderer": "views",
		"subscribers": "messages",
		"typeorm": "database",
		"use-cases": "functions",
		"useCases": "functions",
		"view-models": "views",
		"websockets": "pipe"
	},
	"gruvbox-material-icons.folders.theme": "specific",
	"gruvbox-material-icons.hidesExplorerArrows": false,
	"terminal.integrated.enablePersistentSessions": false,
	"terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
	"terminal.integrated.gpuAcceleration": "on",
	"terminal.integrated.hideOnStartup": "always",
	"update.mode": "manual",
	"window.autoDetectColorScheme": true,
	"window.commandCenter": false,
	"window.customTitleBarVisibility": "never",
	"window.dialogStyle": "native",
	"window.menuBarVisibility": "toggle",
	"window.newWindowDimensions": "inherit",
	"window.newWindowProfile": "Default",
	"window.titleBarStyle": "native",
	"workbench.activityBar.location": "top",
	"workbench.commandPalette.experimental.suggestCommands": true,
	"workbench.editor.customLabels.enabled": true,
	"workbench.editor.customLabels.patterns": {
		"**/.*": "${filename} (${dirname})",
		"**/*": "${filename} (${dirname})",
		"**/*.*": "${filename}.${extname} (${dirname})"
	},
	"workbench.editor.editorActionsLocation": "default",
	"workbench.editor.empty.hint": "hidden",
	"workbench.editor.showTabs": "multiple",
	"workbench.hover.delay": 700,
	"workbench.iconTheme": "gruvbox-material-icons",
	"workbench.layoutControl.enabled": false,
	"workbench.layoutControl.type": "menu",
	"workbench.list.smoothScrolling": true,
	"workbench.panel.defaultLocation": "bottom",
	"workbench.preferredDarkColorTheme": "Catppuccin Mocha",
	"workbench.preferredLightColorTheme": "Catppuccin Latte",
	"workbench.productIconTheme": "fluent-icons",
	"workbench.quickOpen.closeOnFocusLost": false,
	"workbench.reduceMotion": "off",
	"workbench.startupEditor": "none",
	"vscode_custom_css.imports": [
		"file:///${userHome}/.config/not-so-minimal-vscode/styles.css",
		"file:///${userHome}/.config/not-so-minimal-vscode/script.js"
	],
	/* End Not So Minimal - VS Code Settings */

	"[dockerfile]": {
		"editor.defaultFormatter": "ms-azuretools.vscode-docker"
	},
	"[fish]": {
		"editor.defaultFormatter": "ndonfris.fish-lsp"
	},
	"[git-commit]": {
		"editor.rulers": [50, 72],
		"editor.wordWrap": "wordWrapColumn",
		"editor.wordWrapColumn": 72,
		"workbench.editor.restoreViewState": false
	},
	"[json]": {
		"editor.codeActionsOnSave": {
			"source.fixAll": "always"
		},
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.codeActionsOnSave": {
			"source.fixAll": "always"
		},
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[markdown]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.markdownlint": "always"
		},
		"editor.defaultFormatter": "yzhang.markdown-all-in-one"
	},
	"[powershell]": {
		"editor.defaultFormatter": "ms-vscode.powershell",
		"editor.formatOnSave": true,
		"editor.formatOnType": true,
		"editor.rulers": [140],
		"editor.tabSize": 4,
		"files.encoding": "utf8bom"
	},
	"[shellscript]": {
		"editor.defaultFormatter": "foxundermoon.shell-format"
	},
	"[sql]": {
		"editor.defaultFormatter": "mtxr.sqltools"
	},
	"[toml]": {
		"editor.defaultFormatter": "tamasfe.even-better-toml"
	},
	"[typescript]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.ts": "always",
			"source.sortImports": "always"
		},
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[xml]": {
		"editor.defaultFormatter": "redhat.vscode-xml"
	},
	"[yaml]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"bashIde.shellcheckArguments": "--exclude='Makefile,makefile'",
	"cmake.options.statusBarVisibility": "visible",
	"cmake.pinnedCommands": ["workbench.action.tasks.configureTaskRunner", "workbench.action.tasks.runTask"],
	"commentTranslate.hover.concise": true,
	"commentTranslate.hover.string": true,
	"commentTranslate.hover.variable": true,
	"commentTranslate.multiLineMerge": true,
	"commentTranslate.targetLanguage": "pt",
	"continue.telemetryEnabled": false,
	"cSpell.language": "en-US,pt-BR",
